Form\">\r\n                <div class=\"mb-3\">\r\n                  <label for=\"email\" class=\"form-label\">Email Address</label>\r\n                  <input\r\n                    type=\"email\"\r\n                    class=\"form-control\"\r\n                    v-model=\"email\"\r\n                    placeholder=\"Enter your email\"\r\n                    required\r\n                  />\r\n                </div>\r\n                <button type=\"submit\" class=\"btn btn-primary btn-block mt-4\">\r\n                  Submit\r\n                </button>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n</body>\r\n  </template>\r\n  \r\n  <script>\r\n  import axios from 'axios'\r\n  export default {\r\n    data() {\r\n      return {\r\n        email: '',\r\n        errorMessage: '',\r\n        successMessage: '',\r\n      };\r\n    },\r\n    methods: {\r\n        async submitForm() {\r\n      try {\r\n        const response = await axios.post(`/forgot/${this.email}`, { email: this.email });\r\n     \r\n        localStorage.setItem('id', response.data.id);\r\n    console.log(response.data.id);\r\n    this.successMessage = response.data.message;\r\n    // this.$router.push('/updatePassword');\r\n        // Handle the response from the server here\r\n      } catch (error) {\r\n        if (error.response && error.response.data.messages) {\r\n          this.errorMessage = error.response.data.messages.error;\r\n\r\n          setTimeout(() => {\r\n            this.errorMessage = '';\r\n          }, 2000);\r\n        } else {\r\n          this.errorMessage = 'An error occurred while processing your request.';\r\n        }\r\n      }\r\n    },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Add custom styles or override Bootstrap styles here */\r\n  </style>\r\n  "],"mappings":"AAuCE,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,E